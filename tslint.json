{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "no-console": [false],
    "no-implicit-dependencies": [false, "assets"],
    "no-submodule-imports": [false, "babel-plugin-module-resolver"],
    "semicolon": [false],
    "no-any": true,
    "prefer-array-literal": [true, { "allow-type-parameters": true }],
    "variable-name": [true, "ban-keywords", "check-format", "allow-pascal-case", "allow-leading-underscore"]
  }
}
